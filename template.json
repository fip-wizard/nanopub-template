{
  "templateId": "nanopub-template",
  "organizationId": "dsw",
  "version": "1.14.0",
  "name": "Nanopublication Template",
  "description": "Creating nanopublications for submissions",
  "license": "Apache-2.0",
  "metamodelVersion": 16,
  "allowedPackages": [
    {
      "orgId": null,
      "kmId": null,
      "minVersion": null,
      "maxVersion": null
    }
  ],
  "formats": [
    {
      "uuid": "ac187978-c05e-4100-876d-9e59cc0ff0d0",
      "name": "RDF TriG",
      "icon": "fas fa-file",
      "steps": [
        {
          "name": "jinja",
          "options": {
            "content-type": "application/trig",
            "extension": "trig",
            "extras": "submissions",
            "template": "src/trig/index.trig.j2"
          }
        }
      ]
    },
    {
      "uuid": "ac187978-c05e-4100-876d-9e59cc0ff0d1",
      "name": "Plain Preview",
      "icon": "fas fa-file",
      "steps": [
        {
          "name": "jinja",
          "options": {
            "content-type": "text/plain",
            "extension": "trig",
            "extras": "submissions",
            "template": "src/trig/index.trig.j2"
          }
        }
      ]
    },
    {
      "uuid": "ac187978-c05e-4100-876d-9e59cc0ff0d2",
      "name": "HTML Preview",
      "icon": "fas fa-file",
      "steps": [
        {
          "name": "jinja",
          "options": {
            "content-type": "text/html",
            "extension": "html",
            "extras": "submissions",
            "template": "src/html/index.html.j2"
          }
        }
      ]
    }
  ],
  "_tdk": {
    "version": "4.12.0",
    "readmeFile": "README.md",
    "files": [
      "*",
      "!.git/",
      "!.env",
      "!env/"
    ]
  }
}